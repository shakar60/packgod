local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
local uis = game:GetService("UserInputService")

local notif

local connectedKey
local connectedChatted
local selected = _G.packs["med"]
local misc = {}
misc = {
["order"] = 1,
["num"] = #selected,
["afk"] = false,
["funcs"] = {
["random"] = function()
if _G.packEnabled then
event:FireServer(selected[math.random(1, misc["num"])], "All")
end
end,
["sequential"] = function()
if _G.packEnabled then
event:FireServer(selected[misc["order"]], "All")
misc["order"] = (misc["order"] + 1) % misc["num"] + 1
end
end
}
}
misc["selectedFunc"] = misc["funcs"]["random"]
local cmds = {
["set"] = function(args)
local tmp = _G.packs[args[1]]
if tmp then
selected = tmp
misc["num"] = #selected
misc["order"] = 0
print(string.format("changed set to %s.", args[1]))
notif("Trenz Packgod script", string.format("changed set to %s.", args[1]))
end
end,
["order"] = function(args)
local tmp = misc["funcs"][args[1]]
if tmp then
misc["selectedFunc"] = tmp
print(string.format("changed order to %s.", args[1]))
notif("Trenz Packgod Script", string.format("changed order to %s.", args[1]))
end
end,
["interval"] = function(args)
local tmp = tonumber(args[1])
if tmp then
_G.interval = args[1]
print(string.format("changed afk interval to %s.", args[1]))
notif("credits to 2 wo kay", string.format("changed afk interval to %s.", args[1]))
end
end,
["stop"] = function(args)
connectedKey:Stop()
connectedChatted:Stop()
print("terminated twokay's pack script 2.0.")
notif("twokay's pack script 2.0", "terminated twokay's pack script 2.0.")
end,
["enable"] = function(args)
_G.packEnabled = true
print("enabled pack script.")
notif("twokay's pack script 2.0", "enabled pack script.")
end,
["disable"] = function(args)
_G.packEnabled = false
print("disabled pack script.")
notif("twokay's pack script 2.0", "disabled pack script.")
end
}

notif = function(title, desc)
game:GetService("StarterGui"):SetCore("SendNotification", {
Title = title,
Text = desc
})
end
local function cmd(txt)
local split = string.split(txt, " ")

local function setArgsTable()
local second = {}
for i = 2, #split do
table.insert(second, split[i])
end

return second
end

if string.sub(split[1], 1, 1) == _G.packIndex then
local one = cmds[string.sub(split[1], 2, #split[1])]
if one then
one(setArgsTable())
end 
end
end
local function afk()
misc["afk"] = not misc["afk"]

if misc["afk"] then
coroutine.resume(coroutine.create(function()
print(misc["afk"])
while misc["afk"] do
local count = plrs:GetChildren()
local tmp = {"ez", "med"}
local tmpSelected = _G.packs[tmp[math.random(1, 2)]]
local random1 = count[math.random(1, #count)].Name
local random2 = tmpSelected[math.random(1, #tmpSelected)]
if random1 == plr.Name then
random1 = "lol"
end

event:FireServer(string.format("%s %s", random1, random2), "All")

wait(_G.interval)
end
end))
print("enabled afk pack.")
notif("twokay's pack script 2.0", "enabled afk pack.")
else
print("disabled afk pack.")
notif("twokay's pack script 2.0", "disabled afk pack.")
end
end
local function began(i, n)
if i.KeyCode == Enum.KeyCode[_G.packKey] and not n then
misc["selectedFunc"]()
elseif i.KeyCode == Enum.KeyCode[_G.afkKey] and not n then
afk()
end
end

connectedKey = uis.InputBegan:Connect(began)
connectedChatted = plr.Chatted:Connect(cmd)

print(string.format([[
twokay's pack script 2.0 successfully executed!

current prefix: %s
current key: %s
total "pack" count: %i

cmds:
.set (ez/med/long/mom/rap) - changes what type of "jokes" you're going to use.
* ez = short length
* med = medium length
* long = long length
* mom = mom jokes
* rap = rap lyrics
* more coming soon lol
.order (random/sequential) - changes your "packing order."
* random - chooses a random string from set array
* sequential - sequentially goes through each string in set array
.disable - "disables" userinput actions
.enable - "enables" userinput actions
.stop - terminates the script

enjoy packing your "sons," you dirty skids lol

-twokay#2222 <@770482984213413978>
]], _G.packIndex, _G.packKey, #_G.packs["ez"] + #_G.packs["med"] + #_G.packs["long"]))
notif("twokay", "successfully executed!\ncheck console for a list of cmds.")
